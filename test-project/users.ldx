${ this.def("user", this.use("user.ldx")) }
${
    this.onload(() => {
        fetch("https://jsonplaceholder.typicode.com/users")
        .then(e => e.json())
        .then(e => {
            this.def("users", e);
            this.render();
        })
        .catch(e => console.log(e))
    })
}

<ul>
    ${ (this.users && this.users.map) ? this.users.map(e => this.user(e)).join("") : "" }
</ul>