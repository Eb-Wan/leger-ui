<view>
    <main>
        <h1>
            Home
        </h1>
        <section id="userList"></section>
    </main>
</view>

<style>
</style>

<script>
    const userCard = new LgsTemplate($#components/userCard.lgs);

    async function loadUsers() {
        const res = await fetch("https://jsonplaceholder.typicode.com/users");
        userCard.render(await res.json(), document.getElementById("userList"));
    }

    loadUsers();
</script>